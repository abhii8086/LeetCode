import java.util.*;
class Main {
    public static void main(String[] args) {
        int[] arr = {1, 2, 0, 4, 3, 0, 5, 0};
        int n = arr.length;
        int c =0; // count        
        for(int i=0;i<n;i++){
         if(arr[i]!=0)
           arr[c++] = arr[i];
        }
        
        while(c<n)
            arr[c++]=0;
        
        for(int x:arr)
         System.out.print(" "+x);
    }
}
