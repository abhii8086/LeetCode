import java.util.Arrays;
class Main {
    public static void main(String[] args) {
        int[] arr = {-10, -3, -5, -6, -20};
        int n = arr.length;
        Arrays.sort(arr);
        

        int prod1=arr[n-3]*arr[n-2]*arr[n-1];
        int prod2=arr[0]*arr[1]*arr[n-1];
        
        int max = Math.max(prod1,prod2);
        
        System.out.println("max of three: "+max);
    }
}
